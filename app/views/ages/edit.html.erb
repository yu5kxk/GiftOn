<div class="page-title">年齢層編集</div>

<div class="center-block">
	<div class="list-edit-form">
		<%= form_for(@age) do |f| %>
			<table>
				<tr>
					<td class="list-edit-form-name" colspan="2"><%= f.text_field :name, size:'40' %></td>
				</tr>
				<tr>
					<td></td>
					<td class="list-edit-form-btn"><%= f.submit '変更内容を保存', class: "save-btn btn-layout orange-btn" %></td>
				</tr>
			</table>
		<% end %>
	</div>
</div>